{
  "compilerOptions": {
      "outDir": "./dist/",
      "noEmit": true,
      "noImplicitAny": true,
      "module": "commonjs",
      "target": "es6",
      "jsx": "react",
      "resolveJsonModule": true,
      "esModuleInterop": true,
      "isolatedModules": true
  },
  "include": [
      "./__tests__/**/*",
      "./client/**/*",
      "./utils/**/*"
  ],
  "exclude": [
    "node_modules"
  ]
}