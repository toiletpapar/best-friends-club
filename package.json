{
  "name": "best-friends-club",
  "version": "1.0.0",
  "description": "A gathering spot for the best of friends",
  "scripts": {
    "test": "eslint . && jest",
    "init": "npm run buildDev",
    "buildDev": "npm run test && webpack --config webpack.dev.client.js && webpack --config webpack.dev.server.js",
    "build": "npm run test && webpack --config webpack.prod.client.js && webpack --config webpack.prod.server.js",
    "dev": "concurrently --kill-others \"webpack --config webpack.dev.server.js --watch\" \"nodemon ./build/server.bundle.js\"",
    "prod": "node ./build/server.bundle.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/toiletpapar/best-friends-club.git"
  },
  "keywords": [
    "webapp"
  ],
  "author": "Tyler Poon",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/toiletpapar/best-friends-club/issues"
  },
  "homepage": "https://github.com/toiletpapar/best-friends-club#readme",
  "devDependencies": {
